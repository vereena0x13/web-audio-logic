library(quan) {
  cell(BUF) {
    area: 1
    pin(A) {
      direction: input;
    }
    pin(Y) {
      function: "A";
      direction: output;
    }
  }
  
  cell(NOT) {
    area: 1
    pin(A) {
      direction: input;
    }
    pin(Y) {
      function: "A'";
      direction: output;
    }
  }
  
  cell(AND) {
    area: 5
    pin(A) {
      direction: input;
    }
    pin(B) {
      direction: input;
    }
    pin(Y) {
      function: "(A*B)";
      direction: output;
    }
  }

  cell(NAND) {
    area: 4
    pin(A) {
      direction: input;
    }
    pin(B) {
      direction: input;
    }
    pin(Y) {
      function: "(A*B)'";
      direction: output;
    }
  }

  cell(OR) {
    area: 2
    pin(A) {
      direction: input;
    }
    pin(B) {
      direction: input;
    }
    pin(Y) {
      function: "(A+B)";
      direction: output;
    }
  }

  cell(NOR) {
    area: 3
    pin(A) {
      direction: input;
    }
    pin(B) {
      direction: input;
    }
    pin(Y) {
      function: "((A+B)')";
      direction: output;
    }
  }

  cell(XOR) {
    area: 11
    pin(A) {
      direction: input;
    }
    pin(Y) {
      function: "(A^B)";
      direction: output;
    }
    pin(B) {
      direction: input;
    }
  }

  cell(DFF) {
    area: 40
    ff(IQ, IQN) {
      clocked_on: C;
      next_state: D;
    }
    pin(Q) {
      function: "IQ";
      direction: output;
    }
    pin(C) {
      direction: input;
      clock: true;
    }
    pin(D) {
      direction: input;
    }
  }
}